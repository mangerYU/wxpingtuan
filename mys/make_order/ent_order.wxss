.ent_order {
  width: 100%;
  height: 200rpx;
 border-bottom: 2rpx solid rgb(173, 169, 169);
}

.row {
  display: flex;
  flex-direction: row;
  align-items: center;

}

.com {
  display: flex;
  flex-direction: column;
}

.ent_order image {
  width: 150rpx;
  height: 150rpx;
  margin: 30rpx;
}
.butts{
  width: 100%;
  height:70rpx;
  align-items: center;
  justify-content: space-around;
}
.but{
  width:130rpx;
  height: 45rpx;
  font-size: 25rpx;
  background-color: rgb(228, 149, 47);
  border-radius: 30rpx;
  color: #fff;
  margin: 0rpx 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
